      {
        "type": "header",
        "content": "Favicon"
      },
      {
        "type": "checkbox",
        "id": "favicon_enable",
        "label": "Use custom icon"
      },
      {
        "type": "image",
        "id": "favicon.png",
        "label": "Icon",
        "max-width": 32,
        "max-height": 32,
        "info": "32 x 32px .png required"
      }
    ]
