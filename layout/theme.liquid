          {% if settings.footer_social_enable %}
              <div class="grid__item large--one-quarter medium--one-half footer-menu-wrap">
              <h3>{{ 'layout.footer.social_title' | t }}</h3>
                <ul class="inline-list social-icons orange">
                  {% if settings.social_twitter_link != blank %}
                    <li>
                      <a href="{{ settings.social_twitter_link }}" title="{{ shop.name }} on Twitter">
                        Twitter
                      </a>
                    </li>
                  {% endif %}
                  {% if settings.social_facebook_link != blank %}
                    <li>
                      <a href="{{ settings.social_facebook_link }}" title="{{ shop.name }} on Facebook">
                        Facebook
                      </a>
                    </li>
